<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ProfileActivity"
    android:background="@drawable/androidback">

    <TextView
        android:id="@+id/welcome"
        android:layout_width="112dp"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/userwelcome"
        android:layout_marginTop="25dp"
        android:text="WELCOME"
        android:textAlignment="center"
        android:textSize="20sp" />

    <Button
        android:id="@+id/logoutbutton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignTop="@+id/welcome"
        android:layout_marginEnd="31dp"
        android:text="LOGOUT" />

    <TextView
        android:id="@+id/userwelcome"
        android:layout_width="221dp"
        android:layout_height="35dp"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/logoutbutton"
        android:layout_marginStart="22dp" />

    <Button
        android:id="@+id/moviebutton"
        android:layout_width="146dp"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/userwelcome"
        android:layout_marginTop="199dp"
        android:padding="10dp"
        android:text="MOVIE"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/profileemailtext"
        android:layout_width="132dp"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/userwelcome"
        android:layout_marginTop="130dp"
        android:padding="10dp"
        android:text="BOOK NOW"
        android:textAlignment="center"
        android:textSize="20sp" />

    <Button
        android:id="@+id/eventbutton"
        android:layout_width="140dp"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/moviebutton"
        android:layout_alignEnd="@+id/logoutbutton"
        android:text="EVENT"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/text1"
        android:layout_width="145dp"
        android:layout_height="36dp"
        android:layout_alignParentBottom="true"
        android:layout_alignStart="@+id/userwelcome"
        android:layout_marginBottom="220dp" />

    <TextView
        android:id="@+id/text2"
        android:layout_width="145dp"
        android:layout_height="36dp"
        android:layout_alignParentBottom="true"
        android:layout_alignStart="@+id/userwelcome"
        android:layout_marginBottom="174dp" />

    <TextView
        android:id="@+id/text3"
        android:layout_width="142dp"
        android:layout_height="38dp"
        android:layout_alignParentBottom="true"
        android:layout_alignStart="@+id/userwelcome"
        android:layout_marginBottom="127dp" />

    <TextView
        android:id="@+id/text4"
        android:layout_width="142dp"
        android:layout_height="36dp"
        android:layout_alignParentBottom="true"
        android:layout_alignStart="@+id/userwelcome"
        android:layout_marginBottom="80dp" />

    <TextView
        android:id="@+id/text5"
        android:layout_width="142dp"
        android:layout_height="35dp"
        android:layout_alignParentBottom="true"
        android:layout_alignStart="@+id/userwelcome"
        android:layout_marginBottom="33dp" />

    <TextView
        android:id="@+id/text6"
        android:layout_width="137dp"
        android:layout_height="33dp"
        android:layout_alignTop="@+id/text1"
        android:layout_marginEnd="-79dp"
        android:layout_toStartOf="@+id/logoutbutton" />

    <TextView
        android:id="@+id/text7"
        android:layout_width="135dp"
        android:layout_height="35dp"
        android:layout_alignStart="@+id/text6"
        android:layout_alignTop="@+id/text2" />

    <TextView
        android:id="@+id/text8"
        android:layout_width="133dp"
        android:layout_height="39dp"
        android:layout_alignTop="@+id/text3"
        android:layout_marginEnd="-75dp"
        android:layout_toStartOf="@+id/logoutbutton" />

    <TextView
        android:id="@+id/text9"
        android:layout_width="135dp"
        android:layout_height="34dp"
        android:layout_alignTop="@+id/text4"
        android:layout_marginEnd="-77dp"
        android:layout_toStartOf="@+id/logoutbutton" />

    <TextView
        android:id="@+id/text10"
        android:layout_width="127dp"
        android:layout_height="38dp"
        android:layout_alignStart="@+id/eventbutton"
        android:layout_alignTop="@+id/text5" />

    <TextView
        android:id="@+id/text11"
        android:layout_width="139dp"
        android:layout_height="41dp"
        android:layout_alignParentEnd="true"
        android:layout_alignTop="@+id/profileemailtext"
        android:layout_marginEnd="47dp" />

</RelativeLayout>