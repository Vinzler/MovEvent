<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".PVRActivity3"
    android:background="@drawable/androidback">

    <ImageView
        android:id="@+id/seatj7"
        android:layout_width="57dp"
        android:layout_height="60dp"
        android:layout_alignTop="@+id/seati7"
        android:layout_toStartOf="@+id/seath7" />

    <ImageView
        android:id="@+id/seati7"
        android:layout_width="57dp"
        android:layout_height="60dp"
        android:layout_above="@+id/screentext7"
        android:layout_toStartOf="@+id/seatg7" />

    <ImageView
        android:id="@+id/seath7"
        android:layout_width="57dp"
        android:layout_height="60dp"
        android:layout_alignStart="@+id/confirmbutton7"
        android:layout_alignTop="@+id/seatf7" />

    <ImageView
        android:id="@+id/seatg7"
        android:layout_width="57dp"
        android:layout_height="60dp"
        android:layout_alignTop="@+id/seatf7"
        android:layout_centerHorizontal="true" />

    <ImageView
        android:id="@+id/seatf7"
        android:layout_width="57dp"
        android:layout_height="60dp"
        android:layout_alignParentBottom="true"
        android:layout_alignStart="@+id/seata7"
        android:layout_marginBottom="143dp" />

    <ImageView
        android:id="@+id/seate7"
        android:layout_width="57dp"
        android:layout_height="60dp"
        android:layout_alignTop="@+id/seatd7"
        android:layout_toStartOf="@+id/confirmbutton7" />

    <ImageView
        android:id="@+id/seatd7"
        android:layout_width="57dp"
        android:layout_height="60dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="191dp"
        android:layout_toEndOf="@+id/backbutton7" />

    <ImageView
        android:id="@+id/seatb7"
        android:layout_width="57dp"
        android:layout_height="60dp"
        android:layout_alignTop="@+id/seatc7"
        android:layout_centerHorizontal="true" />

    <TextView
        android:id="@+id/silvertext7"
        android:layout_width="wrap_content"
        android:layout_height="40dp"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="210dp"
        android:text="SILVER(Rs100/seat)"
        android:textAlignment="center"
        android:textSize="15sp" />

    <Button
        android:id="@+id/backbutton7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="17dp"
        android:layout_marginTop="23dp"
        android:text="BACK" />

    <Button
        android:id="@+id/confirmbutton7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignTop="@+id/backbutton7"
        android:layout_marginEnd="21dp"
        android:text="CONFIRM" />

    <TextView
        android:id="@+id/goldtext7"
        android:layout_width="wrap_content"
        android:layout_height="40dp"
        android:layout_below="@+id/backbutton7"
        android:layout_centerHorizontal="true"
        android:text="GOLD(Rs200/seat)"
        android:textAlignment="center"
        android:textSize="15sp" />

    <TextView
        android:id="@+id/screentext7"
        android:layout_width="230dp"
        android:layout_height="53dp"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:text="SCREEN"
        android:textAlignment="center"
        android:textSize="30sp" />

    <ImageView
        android:id="@+id/seatc7"
        android:layout_width="57dp"
        android:layout_height="60dp"
        android:layout_alignParentEnd="true"
        android:layout_alignTop="@+id/seata7"
        android:layout_marginEnd="43dp" />

    <ImageView
        android:id="@+id/seata7"
        android:layout_width="57dp"
        android:layout_height="60dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="38dp"
        android:layout_marginTop="121dp" />



</RelativeLayout>